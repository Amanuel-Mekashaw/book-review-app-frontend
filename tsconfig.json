{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "lib": [
      "ES2022",
      "dom"
    ],
    "paths": {
      "@spartan-ng/ui-avatar-helm": [
        ".\\src/lib/ui-avatar-helm/src/index.ts"
      ],
      "@spartan-ng/ui-button-helm": [
        ".\\src/lib/ui-button-helm/src/index.ts"
      ],
      "@spartan-ng/ui-card-helm": [
        ".\\src/lib/ui-card-helm/src/index.ts"
      ],
      "@spartan-ng/ui-checkbox-helm": [
        ".\\src/lib/ui-checkbox-helm/src/index.ts"
      ],
      "@spartan-ng/ui-dialog-helm": [
        ".\\src/lib/ui-dialog-helm/src/index.ts"
      ],
      "@spartan-ng/ui-formfield-helm": [
        ".\\src/lib/ui-formfield-helm/src/index.ts"
      ],
      "@spartan-ng/ui-hovercard-helm": [
        ".\\src/lib/ui-hovercard-helm/src/index.ts"
      ],
      "@spartan-ng/ui-icon-helm": [
        ".\\src/lib/ui-icon-helm/src/index.ts"
      ],
      "@spartan-ng/ui-input-helm": [
        ".\\src/lib/ui-input-helm/src/index.ts"
      ],
      "@spartan-ng/ui-label-helm": [
        ".\\src/lib/ui-label-helm/src/index.ts"
      ],
      "@spartan-ng/ui-pagination-helm": [
        ".\\src/lib/ui-pagination-helm/src/index.ts"
      ],
      "@spartan-ng/ui-popover-helm": [
        ".\\src/lib/ui-popover-helm/src/index.ts"
      ],
      "@spartan-ng/ui-select-helm": [
        ".\\src/lib/ui-select-helm/src/index.ts"
      ],
      "@spartan-ng/ui-separator-helm": [
        ".\\src/lib/ui-separator-helm/src/index.ts"
      ],
      "@spartan-ng/ui-sheet-helm": [
        ".\\src/lib/ui-sheet-helm/src/index.ts"
